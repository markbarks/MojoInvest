<div class="column grid_12">
    <fieldset class="grid_12">
        <legend>Universe</legend>
        <table id="universe_table" summary="Fund Universe" class="grid_11">
            <thead>
            <tr>
                <th scope="col"><input type="checkbox" value="{{symbol}}" name="includeAllFundsCheck"></th>
                <th scope="col">Symbol</th>
                <th scope="col">Name</th>
                <th scope="col">Category</th>
                <th scope="col">Country</th>
                <th scope="col">Index</th>
                <th scope="col">Inception Date</th>
            </tr>
            </thead>
            <tbody>
                {{#funds}}
                <tr>
                    <td class="center"><input type="checkbox" value="{{symbol}}" name="includeFundCheck"></td>
                    <td>{{symbol}}</td>
                    <td>{{name}}</td>
                    <td>{{category}}</td>
                    <td>{{country}}</td>
                    <td>{{index}}</td>
                    <td>{{inceptionDate}}</td>
                </tr>
                {{/funds}}
            </tbody>
        </table>

    </fieldset>
</div>