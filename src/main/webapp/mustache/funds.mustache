<div class="column grid_12">
    <fieldset>
        <legend>Funds</legend>
        <!-- Table markup-->
        <table id="universe-table" summary="Fund Universe" class="sortable">
            <thead>
            <tr>
                <th scope="col"></th>
                <th scope="col">Ticker</th>
                <th scope="col">Name</th>
                <th scope="col">Category</th>
                <th scope="col">Provider</th>
                <th scope="col">Country</th>
                <!--<th scope="col">Index</th>-->
                <!--<th scope="col">Overview</th>-->
                <th scope="col">Inception Date</th>
            </tr>
            </thead>
            <tbody>
            {{#funds}}
            <tr>
                <td class="center"><input type="checkbox" value="{{symbol}}" name="includeFundCheck"></td>
                <td>{{symbol}}</td>
                <td>{{name}}</td>
                <td>{{category}}</td>
                <td>{{provider}}</td>
                <td>{{country}}</td>
                <!--<td></td>-->
                <!--<td></td>-->
                <td>{{inceptionDate}}</td>
            </tr>

            {{/funds}}
            </tbody>
        </table>

    </fieldset>
</div>