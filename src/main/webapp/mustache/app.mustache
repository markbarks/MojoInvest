{{<mustache/super}}
{{$title}}App{{/title}}

{{$content}}

<form>
    <div class="column grid_3">
        <fieldset>
            <legend>Portfolio</legend>
            <ol>
                <li>
                    <label for=initInvest>Initial Investment</label>
                    <input id=initInvest name=initInvest type=number min=1
                           placeholder="ghgfd" required>
                </li>
                <li>
                    <label for=transCost>Transaction Cost</label>
                    <input id=transCost name=transCost type=number min="0">
                </li>
                <li>
                    <label for=portSize>Portfolio Size</label>
                    <input id=portSize name=portSize type=number min="1">
                </li>
                <li>
                    <label for=holdingPeriod>Holding Period</label>
                    <input id=holdingPeriod name=holdingPeriod type=number min="1">
                </li>
            </ol>
        </fieldset>
    </div>
    <div class="column grid_3">
        <fieldset>
            <legend>Relative Strength</legend>
            <ol>
                <li>
                    <input id="maRatioRadio" type="radio" name="rsRadio" value="maRatioRadio"
                           checked/>
                    <label for="maRatioRadio">MA Ratio</label>
                    <select id="ma1" name=Óma1Ó>
                        <option value=Ó4Ó>4</option>
                        <option value=Ó8Ó>8</option>
                        <option value=Ó12Ó selected>12</option>
                        <option value=Ó26Ó>26</option>
                        <option value=Ó39Ó>39</option>
                        <option value=Ó52Ó>52</option>
                    </select>
                    <select id="ma2" name=Óma2Ó>
                        <option value=Ó4Ó>4</option>
                        <option value=Ó8Ó>8</option>
                        <option value=Ó12Ó>12</option>
                        <option value=Ó26Ó selected>26</option>
                        <option value=Ó39Ó>39</option>
                        <option value=Ó52Ó>52</option>
                    </select>
                </li>
                <li>
                    <input id="rocRadio" type="radio" name="rsRadio" value="rocRadio"/>
                    <label for="rocRadio">Rate of Change</label>
                    <select id="roc" name=ÓrocÓ disabled>
                        <option value=Ó4Ó>4</option>
                        <option value=Ó8Ó>8</option>
                        <option value=Ó12Ó>12</option>
                        <option value=Ó26Ó selected>26</option>
                        <option value=Ó39Ó>39</option>
                        <option value=Ó52Ó>52</option>
                    </select>
                </li>
                <li>
                    <input id="alphaRadio" type="radio" name="rsRadio" value="alphaRadio"/>
                    <label for="alphaRadio">Alpha</label>
                    <select id="alpha" name=ÓalphaÓ disabled>
                        <option value=Ó100Ó selected>100</option>
                    </select>
                </li>
            </ol>

        </fieldset>
    </div>
    <div class="column grid_3">
        <fieldset>
            <legend>Risk</legend>
            <ol>
                <li>
                    <label for="stdDevLabel">Adjust by Std Dev</label>
                    <input type="checkbox" id="stdDevLabel" name="stdDevLabel"
                           onclick="goog.dom.getElement('stdDev').disabled =
                                   !goog.dom.getElement('stdDev').disabled"/>
                    <select id="stdDev" name=ÓrocÓ disabled>
                        <option value=Ó4Ó>4</option>
                        <option value=Ó8Ó>8</option>
                        <option value=Ó12Ó>12</option>
                        <option value=Ó26Ó selected>26</option>
                        <option value=Ó39Ó>39</option>
                        <option value=Ó52Ó>52</option>
                    </select>
                </li>
                <li>
                    <label>Trade Equity Curve</label>
                    <input type="checkbox" id="tradeEquityLabel" name="tradeEquityLabel"
                           onclick="goog.dom.getElement('tradeEquity').disabled =
                                   !goog.dom.getElement('tradeEquity').disabled"/>
                    <input id="tradeEquity" name=portSize type=number min="4" disabled value="52">
                </li>
                <li>
                    <label>Use Safe Asset</label>
                    <input type="checkbox" id="safeAssetCheck" name="safeAssetCheck"
                           onclick="goog.dom.getElement('safeAsset').disabled =
                                   !goog.dom.getElement('safeAsset').disabled"/>
                    <input id=safeAsset name=safeAsset type=text disabled>
                </li>
            </ol>
        </fieldset>
    </div>
    <div class="column grid_3">
        <fieldset>
            <legend>Portfolio</legend>
            <ol>
                <li>
                    <div>
                        <label for="fromDateContainer">From Date</label>
                        <span id="fromDateContainer"></span>
                    </div>
                </li>
                <li>
                    <div>
                        <label for="toDateContainer">To Date</label>
                        <span id="toDateContainer"></span>
                    </div>
                </li>
            </ol>
        </fieldset>
    </div>


    <div class="column grid_12">
        <fieldset>
            <legend>Universe</legend>
            <!-- Table markup-->
            <table id="universe-table" summary="Fund Universe" class="sortable">
                <thead>
                <tr>
                    <th scope="col"></th>
                    <th scope="col">Ticker</th>
                    <th scope="col">Name</th>
                    <th scope="col">Category</th>
                    <th scope="col">Provider</th>
                    <th scope="col">Country</th>
                    <!--<th scope="col">Index</th>-->
                    <!--<th scope="col">Overview</th>-->
                    <th scope="col">Inception Date</th>
                </tr>
                </thead>
                <tbody>
                {{#funds}}
                <tr>
                    <td class="center"><input type="checkbox" name="query_myTextEditBox"></td>
                    <td>{{symbol}}</td>
                    <td>{{name}}</td>
                    <td>{{category}}</td>
                    <td>{{provider}}</td>
                    <td>{{country}}</td>
                    <!--<td></td>-->
                    <!--<td></td>-->
                    <td>{{inceptionDate}}</td>
                </tr>

                {{/funds}}
                </tbody>
            </table>

        </fieldset>
    </div>
</form>
{{/content}}

{{$foot-scripts}}
<script>
    App.backtestParams = new BacktestParams({{backtestParams}});
    App.funds = new Funds({{funds}}});
</script>
{{/foot-scripts}}

        {{/mustache/super}}