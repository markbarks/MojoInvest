<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <ui:with type="com.mns.mojoinvest.client.NameTokens" field="nameTokens"/>
    <ui:with field='res' type='com.mns.mojoinvest.client.resources.Resources'/>

    <ui:style>

        /*.params {*/
        /*background-color: #ccccff;*/
        /*}*/

        /*.chart {*/
        /*background-color: #ccccff;*/
        /*}*/

        /*.trades {*/
        /*background-color: #ccccff;*/
        /*}*/


    </ui:style>

    <g:SplitLayoutPanel>
        <g:west size='250'>
            <g:FlowPanel ui:field='params'/>
        </g:west>

        <g:north size='280'>
            <g:FlowPanel ui:field='chart'/>
        </g:north>

        <g:center>
            <g:FlowPanel ui:field='trades'/>
        </g:center>
    </g:SplitLayoutPanel>


</ui:UiBinder>