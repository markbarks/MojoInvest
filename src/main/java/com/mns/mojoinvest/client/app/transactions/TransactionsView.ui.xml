<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:cv="urn:import:com.google.gwt.user.cellview.client"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <ui:with type="com.mns.mojoinvest.client.NameTokens" field="nameTokens"/>
    <ui:with field='res' type='com.mns.mojoinvest.client.resources.Resources'/>


    <ui:style>

        .displayInline {
            display: inline;
        }

        .table {
            width: 100%;
            table-layout: fixed;
        }
    </ui:style>

    <g:HTMLPanel ui:field="container">
        <cv:SimplePager ui:field="pager" stylePrimaryName="{style.displayInline}"
          display="{table}"  location="CENTER"/>

        <cv:CellTable ui:field="table" stylePrimaryName="{style.table}"
                      title="Transactions">
        </cv:CellTable>
    </g:HTMLPanel>

</ui:UiBinder>