<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:cv="urn:import:com.google.gwt.user.cellview.client"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <ui:with type="com.mns.mojoinvest.client.NameTokens" field="nameTokens"/>
    <ui:with field='res' type='com.mns.mojoinvest.client.resources.Resources'/>


    <ui:style>
        .displayInline {
            display: inline;
        }

        .table {
            width: 655px;
            float: right;
            /*table-layout: fixed;*/
        }
    </ui:style>

    <g:HTMLPanel ui:field="container" addStyleNames="{style.table}">
        <cv:CellTable ui:field="table" title="Transactions"/>
        <cv:SimplePager ui:field="pager" display="{table}" location="RIGHT"/>
    </g:HTMLPanel>

</ui:UiBinder>